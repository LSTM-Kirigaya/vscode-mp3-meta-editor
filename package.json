{
    "name": "mp3-meta-editor",
    "displayName": "Mp3 Meta Editor",
    "description": "Editor to edit D3 info in mp3",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.85.0"
    },
    "author": {
        "name": "kirigaya"
    },
    "publisher": "kirigaya",
    "repository": {
        "url": "https://github.com/LSTM-Kirigaya/vscode-mp3-meta-editor"
    },
    "categories": [
        "Programming Languages",
        "Visualization",
        "Education"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "mp3-meta.preview",
                "title": "%mp3-meta.preview%"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "mp3-meta.preview"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.0",
        "@types/node": "16.x",
        "@types/vscode": "^1.85.0",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "@vscode/test-electron": "^2.2.0",
        "eslint": "^8.26.0",
        "glob": "^8.0.3",
        "mocha": "^10.1.0",
        "ts-loader": "^9.4.1",
        "typescript": "^4.8.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "dependencies": {
        "music-metadata": "^10.0.0",
        "vscode-languageserver": "^9.0.1",
        "vscode-languageserver-protocol": "^3.17.5",
        "vscode-languageserver-textdocument": "^1.0.1"
    }
}
